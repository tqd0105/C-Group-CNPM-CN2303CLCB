@page
@model KoiManagement.WebApplication.Pages.Rating_feedback.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Rating</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Rating</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Vet ID</dt>
        <dd class="col-sm-10">@Model.Rating.VetId</dd>

        <dt class="col-sm-2">User ID</dt>
        <dd class="col-sm-10">@Model.Rating.UserId</dd>

        <dt class="col-sm-2">Rating Value</dt>
        <dd class="col-sm-10">@Model.Rating.RatingValue</dd>

        <dt class="col-sm-2">Feedback</dt>
        <dd class="col-sm-10">@Model.Rating.Feedback</dd>

        <dt class="col-sm-2">Created At</dt>
        <dd class="col-sm-10">@Model.Rating.CreatedAt</dd>

        <dt class="col-sm-2">Updated At</dt>
        <dd class="col-sm-10">@Model.Rating.UpdatedAt</dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="Rating.RatingId" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-page="./Index" class="btn btn-secondary">Back to List</a>
    </form>
</div>
