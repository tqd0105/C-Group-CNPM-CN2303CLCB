@page
@model KoiManagement.WebApplication.Pages.Rating_feedback.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>Create Rating</h1>

<hr />
<form method="post">

    <div class="form-group">
        <label asp-for="Rating.RatingId" class="control-label">RatingId</label>
        <input asp-for="Rating.RatingId" class="form-control" placeholder="R***"/>
        <span asp-validation-for="Rating.RatingId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Rating.VetId"></label>
        <input asp-for="Rating.VetId" class="form-control" placeholder="V***" />
        <span asp-validation-for="Rating.VetId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Rating.UserId">CostomerId</label>
        <input asp-for="Rating.UserId" class="form-control" placeholder="C***" />
        <span asp-validation-for="Rating.UserId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Rating.RatingValue"></label>
        <input asp-for="Rating.RatingValue" class="form-control" placeholder="0 to 5" />
        <span asp-validation-for="Rating.RatingValue" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Rating.Feedback"></label>
        <input asp-for="Rating.Feedback" class="form-control" placeholder="texting" />
        <span asp-validation-for="Rating.Feedback" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
    <a asp-page="Index" class="btn btn-secondary">Back to List</a>
</form>


